*.config.js

# Ignorowanie plików, które zawierają poufne informacje (np. hasła)
*.key
*.pem

# Ignorowanie plików związanych z testami
coverage/

# Ignorowanie plików tymczasowych
*.tmp
*.temp

# Ignorowanie plików wynikowych z analizy kodu
*.eslintrc.*.js
